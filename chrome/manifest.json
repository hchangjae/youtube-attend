{
  "manifest_version": 2,

  "name": "Youtube attend checker",
  "description": "Chrome extension for Youtube attend check",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png",
    "128": "icon-128.png"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "activeTab",
    "tabs",
    "declarativeContent",
    "storage",
    "identity",
    "identity.email",
    "contextMenus"
  ]
}
